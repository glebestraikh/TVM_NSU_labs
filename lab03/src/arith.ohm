Arithmetic {
    Sum = NonemptyListOf<Sub, "+">  // один или больше элементов A, разделённых символом +
    Sub = NonemptyListOf<Mul, "-">
    Mul = NonemptyListOf<Div, "*">
    Div = NonemptyListOf<UnaryMin, "/">
    
    UnaryMin = ("-")* Atom 
    
    Atom = 
      | "(" Sum ")" -- parenthesis 
      | number
    
    number = 
        | digit+ -- number 
        | letter+ -- variable
}
